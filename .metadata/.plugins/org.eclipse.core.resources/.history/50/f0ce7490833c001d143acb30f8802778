package one;

public class Hello {

	public static void main(String[] args) {
		// Variavel de numeros inteiros e int:

		@SuppressWarnings("unused")
		int idade = 10;
		int x = 5;
		int y = 7;
		int resultado = x * y;
		System.out.println("o valor de x*y é: " + resultado);

		double salario = 1200.0;
		System.out.println("o salario e: " + salario);
		
		float preco = 22.0f;
		System.out.println("o preço e: " + preco);

		String nome = "Paulo Deyvid";
		System.out.println("Meu nome e: " + nome);
		
		char id = 'p';
		System.out.println("A indentificação desse caractere e:" + id);
		
		System.out.println("O nome dele:  " + nome + "." + nome + "idade" + "anos" );

	}

}
